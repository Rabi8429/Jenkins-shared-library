
@Library('Jenkins-shared-library') _
pipeline{
    agent any
     
    stages{
        stage('Git Checkout'){
            steps{
                gitCheckout(
                    branch: "main",
                    url: "https://github.com/Rabi8429/django-notes-app.git"
                )
            }
        }
        stage('Unit Test maven'){
            steps{

                mvnTest()
            }
        }
    }
}